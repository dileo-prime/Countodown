<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefWidth="160.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="14.0" layoutY="20.0" spacing="5.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="5.0" />
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="129.0" minWidth="10.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="68.0" minWidth="10.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="%Show_Overdues" />
                  <Label text="%Show_Finished" visible="false" GridPane.rowIndex="1" />
                  <CheckBox fx:id="showOverdue" maxWidth="130.0" mnemonicParsing="false" selected="true" GridPane.columnIndex="1" />
                  <CheckBox fx:id="showFinished" maxWidth="128.0" mnemonicParsing="false" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <HBox spacing="5.0" />
            <Label text="%View_Date" />
            <AnchorPane fx:id="viewDateContainer" />
            <Label layoutX="13.0" layoutY="65.0" text="%Maximum_DueDate" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="65.0" />
            <AnchorPane fx:id="maxDateContainer" />
         </children>
      </VBox>
   </children>
</AnchorPane>
